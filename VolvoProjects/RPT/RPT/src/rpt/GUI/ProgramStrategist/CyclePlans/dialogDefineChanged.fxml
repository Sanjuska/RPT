<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="180.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rpt.GUI.ProgramStrategist.CyclePlans.DialogDefineChangedController">
   <children>
      <VBox prefHeight="200.0" prefWidth="400.0">
         <children>
            <Label text="Select which elements represent MAJOR changes">
               <VBox.margin>
                  <Insets left="10.0" top="5.0" />
               </VBox.margin>
            </Label>
            <HBox fx:id="mainContainer" prefHeight="182.0" prefWidth="400.0">
               <children>
                  <VBox fx:id="leftContainer" prefHeight="360.0" prefWidth="300.0" spacing="10.0" HBox.hgrow="ALWAYS">
                     <padding>
                        <Insets left="10.0" top="10.0" />
                     </padding>
                  </VBox>
                  <VBox fx:id="rightContainer" prefHeight="360.0" prefWidth="300.0" spacing="10.0" HBox.hgrow="ALWAYS">
                     <padding>
                        <Insets left="10.0" top="10.0" />
                     </padding>
                  </VBox>
               </children>
            </HBox>
            <HBox nodeOrientation="RIGHT_TO_LEFT" prefHeight="57.0" prefWidth="400.0">
               <children>
                  <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#closeDialog" text="Cancel" HBox.hgrow="NEVER">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" top="20.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="okButton" mnemonicParsing="false" onAction="#okButtonPressed" text="OK">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" top="20.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
