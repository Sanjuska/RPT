<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefWidth="320.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rpt.GUI.ResearchAndDevelopment.DevelopmentProcess.AddGateDialogController">
   <children>
      <VBox maxWidth="1.7976931348623157E308">
         <children>
            <TextField fx:id="gateTextField" maxWidth="1.7976931348623157E308" prefWidth="320.0" promptText="Gate">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </TextField>
            <TextArea fx:id="descriptionTextArea" maxWidth="1.7976931348623157E308" prefHeight="100.0" prefWidth="200.0" promptText="Description">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </TextArea>
            <HBox alignment="BOTTOM_RIGHT" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
               <children>
                  <Button fx:id="addButton" mnemonicParsing="false" onAction="#addItem" onKeyPressed="#keyHandler" prefHeight="27.0" prefWidth="66.0" text="Add">
                     <HBox.margin>
                        <Insets bottom="10.0" top="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="doneButton" mnemonicParsing="false" onAction="#closeDialog" text="Done">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
