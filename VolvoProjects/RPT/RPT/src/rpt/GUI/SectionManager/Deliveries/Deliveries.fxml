                              <Image url="@../../../../Icons/addIcon.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="removeButton" contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" onAction="#removeButtonClicked" text="Remove">
