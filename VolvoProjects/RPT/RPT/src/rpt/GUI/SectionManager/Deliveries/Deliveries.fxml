                           <image>
                              <Image url="@../../../../Icons/removeIcon.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="editButton" contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" text="Edit">
