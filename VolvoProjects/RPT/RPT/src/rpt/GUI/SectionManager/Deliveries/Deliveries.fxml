<BorderPane maxWidth="1.7976931348623157E308" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rpt.GUI.SectionManager.Deliveries.DeliveriesController">
   <top>
      <VBox prefHeight="107.0" prefWidth="600.0" BorderPane.alignment="CENTER">
